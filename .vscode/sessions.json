{
    "$schema": "https://cdn.statically.io/gh/nguyenngoclongdev/cdn/main/schema/v10/terminal-keeper.json",
    "theme": "tribe",
    "active": "default",
    "activateOnStartup": true,
    "keepExistingTerminals": false,
    "sessions": {
        "default": [
            {
                "name": "hello",
                "autoExecuteCommands": true,
                "icon": "person",
                "color": "terminal.ansiGreen",
                "commands": [
                    "echo hello"
                ]
            },
            [
                {
                    "name": "docker:ros",
                    "commands": [
                        ""
                    ]
                },
                {
                    "name": "docker:k8s",
                    "commands": [
                        ""
                    ]
                }
            ],
            [
                {
                    "name": "docker:nats",
                    "commands": [
                        ""
                    ]
                },
                {
                    "name": "docker:fleet",
                    "commands": [
                        ""
                    ]
                }
            ]
        ],
        "Todo": [
            {
                "name": "Run frontend",
                "autoExecuteCommands": true,
                "icon": "device-desktop",
                "color": "terminal.ansiGreen",
                "commands": [
                    "cd frontend;npm run dev"
                ]
            },
            {
                "name": "Compile typescript",
                "autoExecuteCommands": true,
                "icon": "run",
                "color": "terminal.ansiGreen",
                "commands": [
                    "cd backend;npx tsc --watch"
                ]
            },
            {
                "name": "Run backend",
                "autoExecuteCommands": true,
                "icon": "server",
                "color": "terminal.ansiGreen",
                "commands": [
                    "cd backend/compiled-js;npx nodemon app.js"
                ]
            }
        ]
    }
}